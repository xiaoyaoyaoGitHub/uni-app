{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue",
		"^jnpf-(.*)": "@/components/jnpf/jnpf-$1/index.vue"
	},
	"pages": [{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "JNPF快速开发平台",
				"app-plus": {
					"bounce": "none",
					"titleNView": {
						// "type":"transparent",/* 头部渐隐 */
						"coverage": "132px"
					}
				}
			}
		},
		{
			"path": "pages/login/index",
			"style": {
				"navigationBarTextStyle": "white",
				"navigationBarBackgroundColor": "#3281ff",
				"app-plus": {
					"titleNView": false,
					"bounce": "none" // 取消iOS回弹,避免和下拉刷新冲突 (小程序无法取消)
				}
			}
		},
		{
			"path": "pages/index/workFlow",
			"style": {
				"navigationBarTitleText": "协同",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/index/apply",
			"style": {
				"navigationBarTitleText": "办公"
			}
		},
		{
			"path": "pages/index/contacts",
			"style": {
				"navigationBarTitleText": "通讯"
			}
		},
		{
			"path": "pages/index/my",
			"style": {
				"navigationBarTitleText": "我的",
				"navigationBarBackgroundColor": "#ffffff",
				"disableScroll": true
			}
		},
		{
			"path": "pages/form/index"
		}
	],
	"subPackages": [{
			"root": "pages/message",
			"pages": [{
                    "path" : "userDetail/index",
                    "style": {
						"navigationBarTitleText": "",
                    	"navigationBarTextStyle": "white",
                    	"navigationBarBackgroundColor": "#D8D8D8",
                    	"app-plus": {
                    		// "titleNView": false,
                    		"bounce": "none" // 取消iOS回弹,避免和下拉刷新冲突 (小程序无法取消)
                    	}
                    }
                
                }
            ]
		},
		{
			"root": "pages/workFlow",
			"pages": [{
					"path": "flowBefore/index",
					"style": {
						"navigationBarTitleText": ""
					}
				},
				{
					"path": "flowLaunch/index",
					"style": {
						"navigationBarTitleText": "我发起的"
					}
				},
				{
					"path": "flowTodo/index",
					"style": {
						"navigationBarTitleText": "待办事宜"
					}
				},
				{
					"path": "flowDone/index",
					"style": {
						"navigationBarTitleText": "已办事宜"
					}
				},
				{
					"path": "flowCopy/index",
					"style": {
						"navigationBarTitleText": "抄送我的"
					}
				},
				// #ifdef MP
				{
					"path": "filePreview/index",
					"style": {
						"navigationBarTitleText": ""
					}
				},
				// #endif
				{
					"path": "operate/index",
					"style": {
						"navigationBarTitleText": ""
					}
				}
			]
		},
		{
			"root": "pages/apply",
			"pages": []
		},
		{
			"root": "pages/contacts",
			"pages": []
		},
		{
			"root": "pages/my",
			"pages": [{
				"path": "settings/index",
				"style": {
					"navigationBarTitleText": "设置"
				}
			},{
                    "path" : "contactUs/index",
                    "style" :                                                                                    
                {
                    "navigationBarTitleText": "联系我们"
                }
                
                }
                ,{
                    "path" : "abouts/index",
                    "style" :                                                                                    
                {
                    "navigationBarTitleText": "应用介绍",
                    "enablePullDownRefresh": false
                }
                
                },
				{
				    "path" : "yinmaisoft/index",
				    "style" :                                                                                    
				{
				    "navigationBarTitleText": "",
				    "enablePullDownRefresh": false
				}
				
				}
                ,{
                    "path" : "accountSecurity/index",
                    "style" :                                                                                    
                {
                    "navigationBarTitleText": "账号安全",
                    "enablePullDownRefresh": false
                }
                
                }
                ,{
                    "path" : "cancellation/index",
                    "style" :                                                                                    
                {
                    "navigationBarTitleText": "注销账号",
                    "enablePullDownRefresh": false
                }
                
                }
                ,{
                    "path" : "personalData/index",
                    "style" :                                                                                    
                {
                    "navigationBarTitleText": "个人信息",
                    "enablePullDownRefresh": false
					
                }
                
                }
                ,{
                    "path" : "business/index",
                    "style" :                                                                                    
                {
                    "navigationBarTitleText": "企业组织",
                    "enablePullDownRefresh": false
                }
                
                }
            ]
		}
	],
	"preloadRule": {
		// "pages/index/message": {
		// 	"network": "all",
		// 	"packages": ["pages/message"]
		// },
		"pages/index/workFlow": {
			"network": "all",
			"packages": ["pages/workFlow"]
		},
		// "pages/index/apply": {
		// 	"network": "all",
		// 	"packages": ["pages/apply"]
		// },
		// "pages/index/contacts": {
		// 	"network": "all",
		// 	"packages": ["pages/contacts"]
		// },
		"pages/index/my": {
			"network": "all",
			"packages": ["pages/my"]
		}
	},
	"tabBar": {
		"color": "#666666",
		"selectedColor": "#3281ff",
		"backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/index/index",
				"text": "消息",
				"iconPath": "static/image/tabbar/message.png",
				"selectedIconPath": "static/image/tabbar/messageHL.png"
			},
			{
				"pagePath": "pages/index/workFlow",
				"text": "协同",
				"iconPath": "static/image/tabbar/workFlow.png",
				"selectedIconPath": "static/image/tabbar/workFlowHL.png"
			},
			{
				"pagePath": "pages/index/apply",
				"text": "办公",
				"iconPath": "static/image/tabbar/apply.png",
				"selectedIconPath": "static/image/tabbar/applyHL.png"
			},
			{
				"pagePath": "pages/index/contacts",
				"text": "通讯",
				"iconPath": "static/image/tabbar/contacts.png",
				"selectedIconPath": "static/image/tabbar/contactsHL.png"
			},
			{
				"pagePath": "pages/index/my",
				"text": "我的",
				"iconPath": "static/image/tabbar/my.png",
				"selectedIconPath": "static/image/tabbar/myHL.png"
			}
		]
	},
	"globalStyle": {
		"navigationBarTitleText": "JNPF快速开发平台",
		"navigationBarBackgroundColor": "#3281FF",
		"backgroundColor": "#F8F8F8",
		"usingComponents": {
			"ly-tree-node": "/components/ly-tree/ly-tree-node"
		}
	}
}
